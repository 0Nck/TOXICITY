local function LACRZJ_fake_script() -- closeupdate.LocalScript 
	local script = Instance.new('LocalScript', closeupdate)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(LACRZJ_fake_script)()
